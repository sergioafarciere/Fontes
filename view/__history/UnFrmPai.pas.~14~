{$R *.dfm}

procedure TfrmPai.btnIncluirClick(Sender: TObject);
var
Agente : TAgente;
dmconexao : TdmConexao;
begin
  Agente := TAgente.Create;
  dmconexao := TdmConexao.Create(nil);
try
  Agente.ID := dmconexao.gerarId;
finally

end;

end;

