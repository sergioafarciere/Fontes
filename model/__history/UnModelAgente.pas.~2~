unit UnModelAgente;

interface

type TAgente = class
  private
    function getNome: string;
    procedure setNome(const Value: string);
  published
     public
     property Nome : string read getNome write setNome;
     end;

implementation

{ TAgente }

function TAgente.getNome: string;
begin
  getNome := Nome;
end;

procedure TAgente.setNome(const Value: string);
begin
  Value := Nome;
end;

end.
